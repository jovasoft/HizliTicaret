<!DOCTYPE html>

<html lang="en" class="default-style">

<head>
    <title>@ViewData["Title"] - Hızlı Ticaret</title>

    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="IE=edge,chrome=1">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0">
    <link rel="icon" type="image/x-icon" href="favicon.ico">

    <link href="https://fonts.googleapis.com/css?family=Roboto:300,300i,400,400i,500,500i,700,700i,900" rel="stylesheet">

    <!-- Icons. Uncomment required icon fonts -->
    <environment include="Development">
        <!-- <link rel="stylesheet" href="~/assets/vendor/fonts/fontawesome.css" /> -->
        <link rel="stylesheet" href="~/assets/vendor/fonts/ionicons.css" />
        <link rel="stylesheet" href="~/assets/vendor/fonts/linearicons.css" />
        <!-- <link rel="stylesheet" href="~/assets/vendor/fonts/open-iconic.css" /> -->
        <!-- <link rel="stylesheet" href="~/assets/vendor/fonts/pe-icon-7-stroke.css" /> -->
    </environment>
    <environment exclude="Development">
        <!-- <link rel="stylesheet" href="~/assets/vendor/fonts/fontawesome.css" asp-append-version="true" /> -->
        <link rel="stylesheet" href="~/assets/vendor/fonts/ionicons.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/assets/vendor/fonts/linearicons.css" asp-append-version="true" />
        <!-- <link rel="stylesheet" href="~/assets/vendor/fonts/open-iconic.css" asp-append-version="true" /> -->
        <!-- <link rel="stylesheet" href="~/assets/vendor/fonts/pe-icon-7-stroke.css" asp-append-version="true" /> -->
    </environment>


    <!-- Core stylesheets -->
    <environment include="Development">
        <link rel="stylesheet" href="~/assets/vendor/css/bootstrap.css" />
        <link rel="stylesheet" href="~/assets/vendor/css/appwork.css" />
        <link rel="stylesheet" href="~/assets/vendor/css/theme-sunrise.css" />
        <link rel="stylesheet" href="~/assets/vendor/css/colors.css" />
        <link rel="stylesheet" href="~/assets/vendor/css/uikit.css" />
        <link rel="stylesheet" href="~/assets/css/site.css">

    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/assets/vendor/css/bootstrap.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/assets/vendor/css/appwork.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/assets/vendor/css/theme-sunrise.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/assets/vendor/css/colors.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/assets/vendor/css/uikit.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/assets/css/site.css">

    </environment>

    <!-- Layout helpers -->
    <environment include="Development">
        <script src="~/assets/vendor/js/layout-helpers.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/assets/vendor/js/layout-helpers.js" asp-append-version="true"></script>
    </environment>
    <script src="~/assets/vendor/js/pace.js"></script>

    <!-- Libs -->
    <!-- `perfect-scrollbar` library required by SideNav plugin -->
    <environment include="Development"><link rel="stylesheet" href="~/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.css" /></environment>
    <environment exclude="Development"><link rel="stylesheet" href="~/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.min.css" asp-append-version="true" /></environment>

    <!-- Page -->
    <link rel="stylesheet" href="~/assets/vendor/css/pages/authentication.css">

    @RenderSection("Styles", required: false)
</head>

<body>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js" crossorigin="anonymous"></script>

    <div class="page-loader">
        <div class="bg-primary"></div>
    </div>
    <!-- Layout wrapper -->
    <div class="layout-wrapper layout-2">
        <div class="layout-inner">
            @RenderBody()
        </div>
        <!-- Overlay -->
        <div class="layout-overlay layout-sidenav-toggle"></div>
    </div>
    <!-- / Layout wrapper -->

    <!-- Core scripts -->
    <script src="~/assets/vendor/libs/popper/popper.js"></script>
    <script src="~/assets/vendor/js/bootstrap.js"></script>
    <script src="~/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/assets/js/site.js"></script>

</body>

</html>